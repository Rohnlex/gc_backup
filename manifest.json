{
    "manifest_version": 2,
    "name": "Need Backup",
    "version": "0.2",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "permissions": [
      "declarativeContent",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*.gamersclub.com.br/lobby"
        ],
        "js": [
          "content.js", 
          "utils.js"
        ]
      }
    ],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/badfallen16.png",
        "32": "images/badfallen32.png",
        "48": "images/badfallen48.png",
        "128": "images/badfallen128.png"
      }
    },
    "icons": {
      "16": "images/badfallen16.png",
      "32": "images/badfallen32.png",
      "48": "images/badfallen48.png",
      "128": "images/badfallen128.png"
    }
  }
  